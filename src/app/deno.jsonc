{
  "compilerOptions": {
    "lib": [
      "deno.window",
      "deno.unstable",
      "dom"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "noImplicitAny": true
  },
  "importMap": "./import_map.json",
  "lint": {
    "rules": {
      "tags": [
        "recommended"
      ],
      "exclude": [
        "import-prefix-missing"
      ]
    }
  },
  "fmt": {}
}
